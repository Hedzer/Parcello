{
  "name": "parcello",
  "version": "1.3.03",
  "keywords": [
    "cli",
    "build",
    "es6",
    "es2015",
    "gulp",
    "babel",
    "rollup"
  ],
  "description": "Parcello is a build tool for ES6 projects. It uses gulp, babel, rollup and some custom plugins to build code and manage dependencies.",
  "main": "parcello.js",
  "bin": {
    "parcello": "./parcello.js"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Hedzer/Parcello.git"
  },
  "author": "Hedzer Ferwerda",
  "email": "hedzer87@gmail.com",
  "readmeFilename": "README.md",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Hedzer/Parcello/issues"
  },
  "homepage": "https://github.com/Hedzer/Parcello#readme",
  "devDependencies": {},
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "external-helpers"
    ]
  },
  "dependencies": {
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-preset-env": "^1.6.0",
    "capitalize": "^1.0.0",
    "chalk": "^2.1.0",
    "clone": "^2.1.1",
    "commander": "^2.11.0",
    "crc": "^3.4.4",
    "defaults-deep": "^0.2.3",
    "expand-tilde": "^2.0.2",
    "fs-extra": "^2.1.2",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-dereserve": "^0.2.1",
    "gulp-install": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-sourcemaps": "^1.6.0",
    "gulp-uglify": "^2.0.0",
    "inquirer": "^3.2.2",
    "jsonfile": "^3.0.1",
    "linewrap": "^0.2.1",
    "object-path": "^0.11.4",
    "recursive-readdir": "^2.1.1",
    "rollup-plugin-babel": "^2.6.1",
    "rollup-plugin-import-alias": "^1.0.1",
